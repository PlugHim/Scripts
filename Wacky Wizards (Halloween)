local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Wacky Wizards", "BloodTheme")
local Tab = Window:NewTab("Halloween")
local Section = Tab:NewSection("Candy")
Section:NewButton("TP to Cadies (auto clicker on 10sec)", "a", function()
	for i,v in pairs (game:GetService("Workspace").CandySpawns :GetChildren()) do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new (v.Position + Vector3.new (0,0,0))
wait (0.2)
end
end)
local Tab = Window:NewTab("Other")
local Section = Tab:NewSection("Other Stuff")
Section:NewButton("Infinity Yeild", "a", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
Section:NewButton("Rejoin", "a", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/KTollT/Rejoin/main/README.md'))()
end)
local Tab = Window:NewTab("Keybinds")
local Section = Tab:NewSection("Keybinds")
Section:NewKeybind("Press any key", "a", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("Credits: Trouble#4720")
